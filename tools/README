Notes
-----
pacman -Sy vim git openssh
systemctl enable sshd
systemctl start sshd
mkdir ~/clones
cd ~/clones
git clone https://github.com/judge-meister/dotfiles.git dotfiles
./tools/install.sh -i install_yay_from_git
./tools/install.sh -i pacman_settings

# install package.list
yay -Sy $(cat package.list)

systemctl enable ly
systemctl enable bluetooth
systemctl start bluetooth

make

pip3 install psutil beautifulsoup4


